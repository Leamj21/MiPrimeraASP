@model List<MiPrimeraASP.Models.compra>

@{ 
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>reporteCompra</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width">
    <title>JS Bin</title>
    <style>
    </style>
</head>
<body>
    <div class="container">
        <div>
            <h2>Reporte de Compra</h2>
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Fecha</th>
                        <th>Total compra</th>
                        <th>Cliente</th>
                        <th>Usuario</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var element in Model)
                    {
                        <tr>
                            <td>@element.id</td>
                            <td>@element.fecha</td>
                            <td>@element.total</td>
                            <td>@element.id_cliente</td>
                            <td>@element.id_usuario</td>
                        </tr>
                    }
                </tbody>
            </table>
            <button onclick="location.href='https://localhost:44350/Compra/ImprimirReporte'">Imprimir reporte</button>
            <p>
                Este reporte es una prueba y muestra los atributos de la tabla Compra
            </p>
            <img src="data:image/png;base64," alt="Alternate Text" />
        </div>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
</body>
</html>
